<div class="gsl_wyly_tit">
        <h3>在建工地</h3><label style="font-size:14px; color:#333; font-weight:bold; padding:0 5px; float:left;">(<%=@factories.total_entries%>个)</label>
        <%=will_paginate_remote(@factories, :update=>'factories_div', :params => "&id=#{@firm.id}", :url=>"/firms/factory_paginator" ,:page_links => false )%></div>
      <div class='gsl_zjgd'>
        <% if flash[:notice] %><font color="red"><%= flash[:notice] %></font><%end%>
        <table width="100%" border="0" cellspacing="0" cellpadding="0">
  <tr class="gsl_zjgd_tit">
    <td width="90"><strong>所属区域</strong></td>
    <td width="200"><strong>楼盘名称</strong></td>
    <td width="89"><strong>房型</strong></td>
    <td width="52"><strong>预算</strong></td>
    <td width="141"><strong>可供参观时间</strong></td>
    <td width="72"><strong>预约</strong></td>
  </tr>
  <% @factories.each do |factory| %>
    <tr>
      <td width="90"><%=HejiaTag.find(factory.PROVINCE2).TAGNAME%></td>
      <td width="200"><%= factory.NAME %></td>
      <td width="89"><%= factory.FANGXING.blank? ? '--' : factory.FANGXING %></td>
      <td width="52"><%= factory.MONEY.blank? ? '--' : factory.MONEY<<'万' %></td>
      <td width="141"><%= factory.STARTENABLE.strftime("%Y-%m-%d") rescue ""%> 至 <%= factory.ENDENABLE.strftime("%m-%d") rescue ""%></td>
      <td width="72"><a href="javascript:void(0);" title="参观" target="_self" class="gsl_zjgd_btn" onclick="show_advice_div(<%= factory.id %>)">参观</a></td>
    </tr>
  <% end %>
</table>
      </div>
