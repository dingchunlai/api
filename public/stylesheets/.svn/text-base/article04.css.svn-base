
/*标签样式*/
html{ overflow-x:hidden;}
*{margin:0 auto;padding:0;}
body{margin:0; font-family:"宋体"; font-size:12px; line-height:20px;}
html,body { height:100%; font-family:"宋体";} 
a{text-decoration:none;}
a:hover {color:#f60; text-decoration:underline}
img{ border:0px;} 
hr{ height:1px; border:0px solid #ccc;}
hr.dotline_5{ border:0px dotted #ccc;}

/*浮动样式*/
.float_left{ float:left;}

/*对联广告*/
#couplet_right,#couplet_left,#couplet_right_2{ position:absolute; top:-400px;}

/*链接样式*/
.link_14{ font-size:14px; line-height:26px;}
.link_action{ color:#FF6600; text-decoration:none; border:1px solid #ccc; padding:2px 5px 0;}
	.link_action:visited{ color:#FF6600; text-decoration:none;}
	.link_action:hover{ color:#0033CC; text-decoration:none;}
.link_red{ color:#F30; text-decoration:none;}
	.link_red:visited{ color:#F30; text-decoration:none;}
	.link_red:hover{ color:#F30; text-decoration:underline;}
.link_map{ color:#03c; text-decoration:none;}
	.link_map:visited{ color:#03c; text-decoration:none;}
	.link_map:hover{ color:#F30; text-decoration:underline;}
.link_blue{ color:#22a; text-decoration:none;}
	.link_blue:visited{ color:#22a; text-decoration:none;}
	.link_blue:hover{ color:#f30; text-decoration:underline;}
.link_song{ font-family:"宋体"}

